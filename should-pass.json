{
  "compilerOptions": {
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": false,
    "declaration": false,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "skipLibCheck": true,
    "noEmit": true,
    "noEmitOnError": true,
    "module": "commonjs",
    "target": "es5",
    "outDir": "./lib",
    "lib": [
      "es5",
      "scripthost",
      "es2015"
    ]
  },
  "files": [
    "should-pass.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}